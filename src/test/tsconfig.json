{
    "version": "1.8.2",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "sourceMap": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "moduleResolution": "node",
        "outDir": "../../dist/",
        "rootDir": ".."
    },
    "filesGlob": [
        "../../typings/tsd-test.d.ts",
        "**/*.ts"
    ],
    "files": [
        "../../typings/tsd-test.d.ts",
        "test-aplus.ts",
        "test-async.ts",
        "test-BaseError.ts",
        "test-polyfill.ts",
        "test-Promise.ts",
        "test-Stack.ts",
        "test-Trace.ts",
        "test-utils.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
